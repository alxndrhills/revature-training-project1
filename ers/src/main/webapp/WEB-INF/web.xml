<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>ers</display-name>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.revature.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.revature.servlets.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ReimbursementServlet</servlet-name>
		<servlet-class>com.revature.servlets.ReimbursementServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReimbursementServlet</servlet-name>
		<url-pattern>/reimbursements</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>StatusServlet</servlet-name>
		<servlet-class>com.revature.servlets.StatusServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StatusServlet</servlet-name>
		<url-pattern>/statuses</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>TypeServlet</servlet-name>
		<servlet-class>com.revature.servlets.TypeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TypeServlet</servlet-name>
		<url-pattern>/types</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ReimbursementReceiptServlet</servlet-name>
		<servlet-class>com.revature.servlets.ReimbursementReceiptServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReimbursementReceiptServlet</servlet-name>
		<url-pattern>/reimbursements/receipt</url-pattern>
	</servlet-mapping>
</web-app>